<div class="col-md-6">
  <%= form_for  [:admin, weather_collect] do |f| %>
    <div class="form-group">
      <%= f.label "名稱" %>
      <%= f.text_field :name, placeholder: "名稱", class: "form-control"%>
    </div>
    <div class="form-group">
      <%= f.text_field :tag_collection, placeholder: "標籤", class: "form-control"%>
    </div>

    <div class="form-group">
      <%= f.submit "送出", class: "btn btn-primary" %>
    </div>
  <% end %>
</div>